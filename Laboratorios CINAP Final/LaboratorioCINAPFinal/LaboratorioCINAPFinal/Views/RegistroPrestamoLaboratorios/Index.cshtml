@model IEnumerable<LaboratorioCINAPFinal.Models.RegistroPrestamoLaboratorio>

@{
    ViewBag.Title = "Prestamos de Laboratorio";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nuevo Prestamo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaEntrada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaSalida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Asistencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grupo.Materia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Laboratorio.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Maestro.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaSalida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Asistencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grupo.Materia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Laboratorio.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Maestro.Nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_RegistroPrestamoLaboratorio }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_RegistroPrestamoLaboratorio }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_RegistroPrestamoLaboratorio })
        </td>
    </tr>
}

</table>
